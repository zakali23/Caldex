{% extends "admin/base_admin.html.twig"%}
{% block body %}
    <h1>Immeuble</h1>

    <table>
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ immeuble.id }}</td>
        </tr>
        <tr>
            <th>Adresse</th>
            <td>{{ immeuble.adresse }}</td>
        </tr>
        <tr>
            <th>Codepostal</th>
            <td>{{ immeuble.codePostal }}</td>
        </tr>
        <tr>
            <th>Ville</th>
            <td>{{ immeuble.ville }}</td>
        </tr>
        <tr>
            <th>Plan</th>
            <td>{{ immeuble.plan }}</td>
        </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('immeuble_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('immeuble_edit', { 'id': immeuble.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
            <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
